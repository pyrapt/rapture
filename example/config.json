{
  "grammar": "Extended Grammar",
  "syntax": {
    "not_op": "not",
    "and_op": "and",
    "or_op": "or",
    "params_start": "_{",
    "params_stop": "}",
    "paren_left": "(",
    "paren_right": ")",
    "equal_op": "=",
    "not_equal_op": "!=",
    "not_equal_alt_op": "<>",
    "less_than_op": "<",
    "less_than_equal_op": "<=",
    "greater_than_op": ">",
    "greater_than_equal_op": ">=",
    "select_op": "\\s",
    "project_op": "\\p",
    "rename_op": "\\r",
    "assign_op": ":=",
    "join_op": "\\j",
    "theta_join_op": "\\theta_join",
    "natural_join_op": "\\natural_join",
    "union_op": "+",
    "difference_op": "-",
    "intersect_op": "\\i"
  }
}